{
  "name": "respec-kik-v-dev",
  "version": "0.1.0",
  "description": "ResPec template compilet for Markdown Content application development in TypeScript",
  "scripts": {
    "build": "npm run build:lib && npm run build:cli",
    "build:cli": "tsc -p ts-cli-config.json",
    "build:lib": "rollup -c",
    "test": "ts-node -P ts-node-config.json bin/respecmd-cli.ts ../template test test-output"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onnohaldar/respec-kik-v.git"
  },
  "keywords": [
    "fuseki",
    "cli",
    "development",
    "typescript"
  ],
  "author": "Onno Haldar <onno@haldar.nl>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/onnohaldar/respec-kik-v/issues"
  },
  "homepage": "https://github.com/onnohaldar/respec-kik-v#readme",
  "devDependencies": {
    "@types/cpx": "^1.5.1",
    "@types/node": "^14.14.31",
    "rollup": "^2.40.0",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.2"
  },
  "dependencies": {
    "colors": "^1.4.0",
    "cpx": "^1.5.0",
    "markdown-to-json": "^0.5.4"
  }
