{
  "name": "respec-tools",
  "version": "0.8.0",
  "description": "ResPec Tools to parse and reuse templates etc.",
  "main": "dist/lib/index.js",
  "module": "dist/lib/index.esm.js",
  "bin": {
    "respecmd": "dist/bin/respecmd-cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onnohaldar/respec-tools.git"
  },
  "keywords": [
    "fuseki",
    "cli",
    "typescript"
  ],
  "author": "Onno Haldar <onno@haldar.nl>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/onnohaldar/respec-tool/issues"
  },
  "homepage": "https://github.com/onnohaldar/respec-tool#readme",
  "scripts": {
    "build": "npm run build:lib && npm run build:cli",
    "build:cli": "tsc -p ts-cli-config.json",
    "build:lib": "rollup -c",
    "test": "ts-node -P src/ts-node-config.json src/bin/respecmd-cli.ts test-input/template test-input/content test-output"
  },
  "devDependencies": {
    "@types/cpx": "^1.5.1",
    "@types/node": "^14.14.31",
    "rollup": "^2.40.0",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.2"
  },
  "dependencies": {
    "cpx": "^1.5.0"
  }
}
